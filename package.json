{
  "name": "nodejs-server-starter",
  "version": "0.0.1",
  "description": "Simple NodeJS Server Starter",
  "repository": "git@github.com:khang859/nodejs-server-starter.git",
  "author": "Khang Nguyen",
  "license": "MIT",
  "main": "dist/index.js",
  "private": true,
  "dependencies": {
    "argon2": "^0.41.1",
    "drizzle-orm": "^0.38.3",
    "fastify": "^5.2.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "pg": "^8.13.1",
    "tsc-alias": "^1.8.10",
    "zod": "^3.24.1"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "start": "node --env-file=.env dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@jest/types": "^29.6.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.3",
    "@types/pg": "^8.11.10",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  }
}
